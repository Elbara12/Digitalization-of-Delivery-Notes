{
  "name": "wp-mvc",
  "version": "1.0.0",
  "description": "Simple MVC-style contacts manager API using Node.js, Express and MySQL (Docker-ready).",
  "main": "index.js",
  "scripts": {
    "start": "node --watch src/index.js",
    "test": "jest --coverage"
  },
  "author": "Matteo Baraggini",
  "license": "MIT",
  "keywords": [
    "node",
    "express",
    "mysql",
    "docker",
    "mvc",
    "api"
  ],
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/SRC/infrastructure/web/utils/",
      "/SRC/infrastructure/web/services/"
    ]
  },
  "dependencies": {
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "mysql2": "^3.13.0",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.0",
    "express-validator": "^7.0.1",
    "multer": "1.4.5-lts.1",
    "axios": "^1.4.0",
    "form-data": "^4.0.0",
    "nodemailer": "6.9.4",
    "googleapis": "^139.0.0",
    "winston": "^3.8.2",
    "pdfkit": "^0.15.1",
    "get-stream": "6.0.1",
    "@pinata/sdk": "^2.0.0",
    "jest": "29.7.0",
    "swagger-jsdoc": "^6.2.0",
    "swagger-ui-express": "^4.6.2"
  }
}